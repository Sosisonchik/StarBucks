<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sosiisonchik.starbucks">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/starbucks"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/starbucks"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CoffeeActivities.CoffeeActivity" />
        <activity android:name=".CoffeeActivities.CoffeInActivity" />
        <activity android:name=".Food.FoodActivity" />
        <activity android:name=".Food.FoodInActivity"></activity>
    </application>

</manifest>